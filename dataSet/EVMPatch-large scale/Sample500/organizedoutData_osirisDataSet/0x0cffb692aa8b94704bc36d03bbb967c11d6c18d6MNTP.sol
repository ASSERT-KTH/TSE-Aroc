pragma solidity ^0.4.16;

contract MNTP {
    string name = "Goldmint MNT Prelaunch Token";
    string symbol = "MNTP";
    uint decimals = 18;
    address creator = 0x0;
    address icoContractAddress = 0x0;
    bool lockTransfers = false;
    uint TOTAL_TOKEN_SUPPLY = 10000000 * 1 ether;
    enum State{Init, ICORunning, ICOPaused, ICOFinished, Refunding, Migrating}
    function issueTokens(address _who, uint _tokens) public view {
                  require((totalSupply + _tokens>=totalSupply) && (totalSupply + _tokens>=_tokens));

    }

}
