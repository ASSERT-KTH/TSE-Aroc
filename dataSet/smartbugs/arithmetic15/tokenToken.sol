pragma solidity ^0.4.18;

contract Token {
    mapping(address => uint) balances;
    uint totalSupply;
    function transfer(address _to, uint _value) public view {
                  require(balances[msg.sender]>=_value);
      if( balances[msg.sender] -  _value>=0){
          require(balances[msg.sender]>=_value);
        }


    }

}
